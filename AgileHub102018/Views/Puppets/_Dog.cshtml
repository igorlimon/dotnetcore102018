@model AgileHub102018.Models.Puppets.Dog

<div>
    <h4>Dog </h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.BirthDay)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.BirthDay)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.DogAddress)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DogAddress)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Vaccines)
        </dt>
        <dd>
            @foreach (var vaccine in Model.Vaccines)
            {
                <span>@vaccine</span> <br/>
            }
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new {  id = Model.Id  }) |
</div>
